<Window x:Class="ArendWpfApp.ManagerMainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:ArendWpfApp"
        mc:Ignorable="d"
        Title="ManagerMainWindow" Height="450" Width="800"  WindowStartupLocation="CenterScreen">
    <Grid>
        <StackPanel HorizontalAlignment="Center" VerticalAlignment="Center">
            <Button x:Name="ProfileShowButton" Content="Профиль" Click="ProfileShowButton_Click" />
            <Button x:Name="CarsShowButton" Content="Машины" Click="CarsShowButton_Click"  />
            <Button x:Name="ClientsShowButton" Content="Клиенты" Click="ClientsShowButton_Click" />
            <Button x:Name="SkidkiShowButton" Content="Скидки" Click="SkidkiShowButton_Click" />
        </StackPanel>
    </Grid>
</Window>
